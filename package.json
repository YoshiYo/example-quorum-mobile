{
  "name": "quorum-mobile",
  "version": "0.0.0-development",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "detox": "^8.1.1",
    "husky": "^0.14.3",
    "jest-expo": "~27.0.0",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1",
    "semantic-release": "^15.9.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest",
    "semantic-release": "semantic-release",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "^27.0.1",
    "react": "16.3.1",
    "react-native": "~0.55.2"
  },
  "release": {
    "branch": "master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YoshiYo/example-quorum-mobile.git"
  },
  "detox": {
    "test-runner": "jest",
    "specs": "e2e",
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/testing-react-e2e.app",
        "build": "xcodebuild -project ios/testing-react-e2e.xcodeproj -scheme testing-react-e2e -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  }
}
